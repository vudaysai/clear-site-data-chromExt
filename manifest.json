{
  "name": "Clear Site Data ",
  "description": "This will clear active chrome tab localStorage, sessionStorage and cookies",
  "version": "0.0.1",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "storage",
    "cookies",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/icon32.png" 
  }
}
